server {
    listen 80;
    listen [::]:80 default_server;
    root   /usr/share/nginx/html;
    index  index.html index.htm;

    location /redirect_me {
        return 301 http://www.my80stv.com;
    }

    error_page 404 /404.html;
    location /404 {
        root /usr/share/nginx/html;
        internal;
    }
}tee/etc/nginx/sites-available/default